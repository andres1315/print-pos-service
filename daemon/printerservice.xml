<service>
	<id>printerservice.exe</id>
	<name>PrinterService</name>
	<description>Servidor local de impresión térmica</description>
	<executable>C:\Program Files\nodejs\node.exe</executable>
	<argument>D:\Dev\printerwindows\node_modules\node-windows\lib\wrapper.js</argument>
	<argument>--file</argument>
	<argument>D:\Dev\printerwindows\index.js</argument>
	<argument>--scriptoptions=</argument>
	<argument>--log</argument>
	<argument>PrinterService wrapper</argument>
	<argument>--grow</argument>
	<argument>0.25</argument>
	<argument>--wait</argument>
	<argument>1</argument>
	<argument>--maxrestarts</argument>
	<argument>3</argument>
	<argument>--abortonerror</argument>
	<argument>n</argument>
	<argument>--stopparentfirst</argument>
	<argument>undefined</argument>
	<logmode>rotate</logmode>
	<stoptimeout>30sec</stoptimeout>
	<serviceaccount>
		<domain>DUKE</domain>
		<user>LocalSystem</user>
		<password></password>
		<allowservicelogon>true</allowservicelogon>
	</serviceaccount>
	<workingdirectory>D:\Dev\printerwindows</workingdirectory>
</service>